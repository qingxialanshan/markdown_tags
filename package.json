{
    "name": "gotodef",
    "displayName": "GoToDef",
    "description": "Go to Definication",
    "version": "0.0.1",
    "publisher": "amyl",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.goToDef"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.goToDef",
            "title": "Go to definition"
        }],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.goToDef",
                    "when":"editorHasSelection && editorLangId==markdown",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}